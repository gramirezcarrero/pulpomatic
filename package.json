{
  "name": "pulpomatic",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "start": "node build/src/server.js",
    "dev": "nodemon src/server.ts -e ts,yml",
    "lint": "eslint . --ext .ts --fix",
    "debug": "nodemon --watch src/**/* -e ts,json --exec node --inspect-brk -r ts-node/register src/server.ts",
    "1pretest": "eslint . --ext .ts",
    "test": "ENV_FILE=./config/.env.test  && NODE_ENV=test && jest --verbose false --coverage --forceExit --reporters=default --reporters=jest-junit --detectOpenHandles --runInBand"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "testTimeout": 8000
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@meanie/joi-object-id": "^2.2.1",
    "async-middleware": "^1.2.1",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-http-context": "^1.2.4",
    "http": "0.0.1-security",
    "joi": "^17.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.0",
    "serialize-error": "^8.0.1",
    "swagger-ui-express": "^4.1.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/base-64": "^0.1.3",
    "@types/config": "0.0.36",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.8",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.13",
    "@types/js-yaml": "^3.12.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.162",
    "@types/mongoose": "^5.7.36",
    "@types/multer": "^1.4.4",
    "@types/node": "^14.6.4",
    "@types/node-cron": "^2.0.3",
    "@types/saml2-js": "^1.6.8",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
